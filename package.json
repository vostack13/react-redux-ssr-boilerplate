{
  "name": "react-redux-ssr-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate with Redux and MongoDB",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-client": "webpack --config config/webpack.client.js --watch",
    "dev:build-server": "webpack --config config/webpack.server.js --watch",
    "dev:server": "nodemon --watch build --exec node build/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vostack13/react-redux-ssr-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "ssr",
    "mongodb",
    "nodejs",
    "jwt"
  ],
  "author": "Stanislav Vozikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vostack13/react-redux-ssr-boilerplate/issues"
  },
  "homepage": "https://github.com/vostack13/react-redux-ssr-boilerplate#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.0.2",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.0",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  }
}
